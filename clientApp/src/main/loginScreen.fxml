<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="login.LoginController">
   <center>
      <AnchorPane prefHeight="591.0" prefWidth="950.0">
         <children>
            <Button fx:id="loginButton" layoutX="440.0" layoutY="364.0" mnemonicParsing="false" onAction="#activateLoginButton" prefHeight="57.0" prefWidth="110.0" text="Login" />
            <Label fx:id="userNameLabel" alignment="TOP_CENTER" layoutX="91.0" layoutY="250.0" prefHeight="24.0" prefWidth="122.0" text="User Name :" />
            <TextField fx:id="userNameTextField" layoutX="259.0" layoutY="249.0" onAction="#activateUserNameTextField" prefHeight="25.0" prefWidth="471.0" />
            <Label fx:id="errorMessageLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="260.0" layoutY="313.0" prefHeight="17.0" prefWidth="469.0" textAlignment="CENTER" />
            <Label fx:id="helloUserLabel" alignment="TOP_LEFT" contentDisplay="TOP" layoutX="14.0" layoutY="9.0" prefHeight="25.0" prefWidth="208.0" text="Label" textAlignment="CENTER" />
            <Label fx:id="absLabel" alignment="TOP_CENTER" layoutX="302.0" layoutY="9.0" prefHeight="56.0" prefWidth="478.0" text="Alternative Bank System App" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
